{
  "name": "grizzy",
  "version": "0.1.1",
  "description": "common visualization functionality",
  "main": "src/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json src/*.ts",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "prelib": "rm -rf lib/ && mkdirp lib/",
    "mocha": "mocha --compilers js:babel-register test",
    "test:node": "mocha test/**.test.js -w",
    "postdist": "uglifyjs dist/cycle-base.js -o dist/cycle-base.min.js",
    "version-patch": "npm version patch --message 'chore(version): %s'",
    "version-minor": "npm version minor --message 'chore(version): %s'",
    "version-major": "npm version major --message 'chore(version): %s'",
    "postversion": "npm publish --access=public",
    "build": "npm-run-all --parallel build:*",
    "test": "./node_modules/.bin/webpack --config webpack.test.config.js --watch",
    "dev": "./node_modules/.bin/webpack-dev-server --hot --config webpack.test.config.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base",
    "build:dev": "webpack --watch"
  },
  "keywords": [
    "d3",
    "reactive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesrhaley/grizzy"
  },
  "dependencies": {
    "d3": "3.5.15",
    "rx": "4.0.6"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.5.0",
    "glob": "7.1.1",
    "mocha": "3.1.2",
    "run-all": "1.0.1",
    "rx": "4.0.6",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  },
  "author": "James Haley",
  "license": "MIT"
}
